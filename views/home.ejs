<%posts.forEach(function(post){%>
    <div class="posts">
        <h1><%= post.title  %></h1>
        <p><%= post.content.substring(0, 345) %>... <a href="/posts/<%=post.title%>">Read more</a></p>    
    </div>
<%})%>

